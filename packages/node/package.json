{
  "name": "@namespace/node",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node dist/code/index.js",
    "clean": "rm -rf dist/code && rm -f tsconfig.tsbuildinfo && mkdir dist/code && echo \"cleaned dist/code\"",
    "clean-zip": "rm -f node.zip && echo \"cleaned node.zip\"",
    "prepack": "yarn build",
    "build": "yarn clean && yarn clean-zip && yarn compile && yarn zip && echo \"zipped node\"",
    "compile": "tsc && cp \"./package.json\" ./dist/code/ && echo \"compiled\"",
    "zip": "zip ../webportal/public/node.zip -9r ./dist/ && echo \"zipped\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "dev": "nodemon ./src/index.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
